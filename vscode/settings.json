{
  // Controls the font size in pixels.
  "editor.fontSize": 14,
  // Controls the font family.
  "editor.fontFamily": "'JetBrainsMono-Regular', FiraCode-Regular,OperatorMono-Medium, Menlo, Monaco, 'Courier New', monospace",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "editor.formatOnPaste": true,
  
  "breadcrumbs.enabled": true,
  
  "explorer.compactFolders": false,

  "workbench.startupEditor": "newUntitledFile",
  "editor.minimap.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.largeFileOptimizations": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.fontLigatures": true,
  "files.autoSave": "afterDelay",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  // Terminal configuration
  "terminal.integrated.fontWeightBold": "normal",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "FiraMonoForPowerline-Medium",
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.cursorBlinking": true,
  "debug.node.autoAttach": "on",
  "search.exclude": {
    "**/bower_components": false,
    "**/bower_componentsl": true
  },
  "editor.renderWhitespace": "none",
  "explorer.confirmDelete": false,
  "python.languageServer": "Microsoft",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.colorCustomizations": {
    // "terminal.foreground": "#839496",
    // "terminal.background": "#002833",
    // "terminal.ansiBlack": "#003541",
    // "terminal.ansiBlue": "#268bd2",
    // "terminal.ansiCyan": "#2aa198",
    // "terminal.ansiGreen": "#859901",
    // "terminal.ansiMagenta": "#d33682",
    // "terminal.ansiRed": "#ca4846",
    // "terminal.ansiWhite": "#eee8d5",
    // "terminal.ansiYellow": "#b58901",
    // "terminal.ansiBrightBlack": "#586e75",
    // "terminal.ansiBrightBlue": "#839496",
    // "terminal.ansiBrightCyan": "#93a1a1",
    // "terminal.ansiBrightGreen": "#586e75",
    // "terminal.ansiBrightMagenta": "#6c6ec6",
    // "terminal.ansiBrightRed": "#cb4b16",
    // "terminal.ansiBrightWhite": "#fdf6e3",
    // "terminal.ansiBrightYellow": "#657b83",
    // "terminalCursor.foreground": "#839496",
    // "terminalCursor.background": "#003541"
  },
  "editor.bracketPairColorization.enabled": true,
  "debug.javascript.autoAttachFilter": "disabled",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.tabSize": 2,
  "workbench.colorTheme": "Webstorm IntelliJ Darcula Theme",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
